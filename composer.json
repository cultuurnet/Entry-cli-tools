{
    "name": "cultuurnet/entry-console",
    "description": "Command line interface tools for Entry",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "cultuurnet/entry": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\EntryConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\EntryConsole\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "bin": [
        "bin/entry"
    ]
}
